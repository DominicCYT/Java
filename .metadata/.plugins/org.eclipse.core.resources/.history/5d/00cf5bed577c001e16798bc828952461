/**
 * 
 */
package p1;

/**
 * 
 */
public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		System.out.println("Hello, world");
		
		int a = 3;
		int b = 9;
		int c = 7;
		double w = 12.9;
		double y = 3.2;
		
		System.out.println("3 + 9 * 7 = " + (a+b*c));
		System.out.println("3 - 9 - 7 = " + (a-b-c));
		System.out.println("3 / 9 = " + (a/b));
		System.out.println("9 / 3 = "+(b/a));
		System.out.println("3 - 9 / 7 = "+(a-b/c));
		System.out.println("12.9 / 3.2 = "+(w/y));
		System.out.println("3.2 / 12.9 = "+(y/w));
		System.out.println("3 + 12.9 / 9 = "+(a+w/b));
		System.out.println("3 % 9 / 3.2 = "+(a%b/y));
		System.out.println("9 % 3 = "+(b%a));
		System.out.println("12.9 % 3.2 = "+(w%y));

	}

}
